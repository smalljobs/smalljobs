= link_to broker_dashboard_path do
  = t('navigation.dashboard')
%strong
  = " >  Jugendliche_r"
%div.row
  .col-sm-8
    = render 'form'
  .col-sm-4
    .row
      .col-sm-12.margin_top_20.text-center
        = link_to "#", class: "button btn btn-primary btn-success js-rocket-chat-start" do
          Chat (4)
        #js-chat-seeker-container.sj-chat-seeker-container.margin_top_20 
          #js-chat-seeker  
      .col-sm-12.bg-allocations.margin_top_20    
        .bg-allocations  
          - if @seeker.allocations.where.not(job: nil).exists?
            %div.provider_allocations
              = render 'allocations'
          = render 'todos'
          = render 'assignments'

= render partial: '/rocketchats/rc_seeker', locals: {seeker: @seeker}

